{
  "name": "rotel-agent",
  "version": "0.0.10",
  "bin": "lib/index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "dev": "yarn build && node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streamfold/rotel-nodejs"
  },
  "author": "Ray Jenkins <ray@streamfold.com>",
  "bugs": {
    "url": "https://github.com/streamfold/rotel-nodejs/issues"
  },
  "homepage": "https://github.com/streamfold/rotel-nodejs#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "typescript": "^4.9.4"
  },
  "optionalDependencies": {
    "rotel-agent-linux-x64": "0.0.10",
    "rotel-agent-linux-arm64": "0.0.10",
    "rotel-agent-darwin-arm64": "0.0.10"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "lib/*"
    ],
    "root": true
  }
}
